class Test1
{
 public static void main(String []args)
	{
      int a[]={2,3,4,5,6,8,7};
	  int b[]=new int[a.length];
	  int num=1;
	  for (int i=0;i<a.length;i++ )
	  {
		  if (num<a.length)
		  {
		  
		  b[i]=a[num];
		   num++;
		  }
		  else
		  {
			  b[i]=a[0];
		  }
		  
	  }
	  for (int j=0;j<a.length ;j++ )
	  {
       System.out.print(a[j]+" ");
	  }
	  System.out.println();
	   for (int j=0;j<a.length ;j++ )
	  {
       System.out.print(b[j]+" ");
	  }
   }
}

