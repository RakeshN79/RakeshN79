class OddDigits
{
	public void display()
	{
		int a=0;
		int b=1;
		int result=0;
		for (int i=1;i<10 ;i++ )
		{
			int sum=a+b;
			if (sum%2!=0)
			{
				result=result+sum;
				System.out.println("odd value......... ..."+sum);
				
			}
			a=b;
			b=sum;

		}
		System.out.print("summation of odd fibancciii............."+result);
		
	}
}
class MainClass4
{
 public static void main(String []args)
	{
         OddDigits obj=new OddDigits();
		 obj.display();

	     
	  }
	
}
