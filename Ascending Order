class AscendingOrder
{
	
	public static void main(String[]args)
	{
		int a[]={6,4,8,7,2,5};
		for (int j=0;j<a.length ;j++ )
		{
		int index=1;
		for (int i=0;i<a.length ;i++ )
		    {
			int temp=0;
			int temp1=0;
			if (index<a.length)
			  {

			if (a[i]>a[index])
			    {
               temp=a[i];
			   temp1=a[index];
			   a[i]=temp1;
			   a[index]=temp;

			}
              index++;
			}	
			
		}
			
		
		}
	for (int i=0;i<a.length ;i++ )
	{
		System.out.println(a[i]);
	}
		
	}
}

